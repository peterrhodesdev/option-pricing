{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "overrides": [
      {
        "files": ["test/**/*.test.js"],
        "env": {
          "jest": true
        }
      }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
      "jest",
      "prettier"
    ],
    "rules": {
      "import/no-default-export": "error",
      "import/prefer-default-export": "off",
      "prettier/prettier": ["error"]
    }
}
