{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "eslint-config-prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
      "eslint-plugin-prettier"
    ],
    "rules": {
      "prettier/prettier": ["error"]
    }
}
